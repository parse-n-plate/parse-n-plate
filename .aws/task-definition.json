{
  "family": "parse-n-plate-task-prod",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "parse-n-plate-container-prod",
      "image": "<ECR_REGISTRY>/parse-n-plate:<IMAGE_TAG>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}